#include <stdio.h>

int main()
{
    int s1,s2,s3;
    printf("Enter three sides of a triangle:");
    scanf("%d %d %d", &s1 , &s2 , &s3);
    if(s1==s2 && s2==s3){
        printf("\nEquilateral triangle");
    }
    else if(s1==s2 || s2==s3 || s1==s3){
        printf("\nIsosceles triangle");
    }
    else{
        printf("\nScalene triangle");
    }
       return 0;
}
