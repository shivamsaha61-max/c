#include <stdio.h>
#include <math.h>

int main() {
    int num;
    double root;
    printf("Enter an integer: ");
    scanf("%d", &num);

    root = sqrt(num + 1);

    if (root == (int)root)
        printf("%d is a Sunny number.\n", num);
    else
        printf("%d is not a Sunny number.\n", num);

    return 0;
}
