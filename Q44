#include <stdio.h>

int main() {
    int num, i, sum = 0;
    printf("Enter an integer: ");
    scanf("%d", &num);
    for (i = 1; i <= num / 2; i++) {
        if (num % i == 0)
            sum += i;
    }
    if (sum > num)
        printf("%d is an Abundant number.\n", num);
    else
        printf("%d is not an Abundant number.\n", num);

    return 0;
}
