#include <stdio.h>
int main()
{
    int num,sum=0;
    int temp;
    printf("\nEnnter a positive integer");
    scanf("%d",&num);
     temp=num;
    while(temp>0)
    { 
        int digit=temp%10;
        sum=sum+digit;
        temp=temp/10;
        
    }
    if (num%sum==0)
    {
        printf("Niven number");
    }
    else
    {
        printf("not niven number");
        
    }
    return 0;
    }
