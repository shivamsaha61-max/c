int main() {
    int n;
    unsigned long long factorial = 1;
    printf("Enter a positive integer: ");
    scanf("%d", &n);
    if (n < 0) {
        printf("Invalid input. Please enter a positive integer.\n");
        return 1;
    }
    else{
    for (int i = 1; i <= n; ++i) {
        factorial *= i;
    }

    printf("Factorial  is  %d", factorial);
}
    return 0;
}
